<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel styleName="estilo-backgroun-login" width="1000" height="500">
		<g:AbsolutePanel height="135px" width="305px" styleName="estilo-login"  >
			<g:at left="59" top="19">
				<g:Label text="Login :"/>
			</g:at>
			<g:at left="104" top="19">
				<g:TextBox width="151px" height="8px" ui:field="txtLogin"/>
			</g:at>
			<g:at left="33" top="49">
				<g:Label text="Password :"/>
			</g:at>
			<g:at left="104" top="45">
				<g:PasswordTextBox width="151px" height="8px" ui:field="txtPass"/>
			</g:at>
			<g:at left="133" top="97">
				<g:Button width="65px" height="28px" text=" Sign In" ui:field="btnLogin" styleName="estilo-button-login"/>
			</g:at>
			<g:at left="68" top="75">
				<g:Label ui:field="lblmsn"/>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 